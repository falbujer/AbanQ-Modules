<!DOCTYPE TMD>
<TMD>
	<name>tt_compare</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Comparación de dos valores")</alias>
	<!--Tabla de soporte para el formulario de comparación de valores-->

	<field>
		<name>idcompare</name>
		<!--Identificador interno-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>tipo1</name>
		<!--Tipo del valor 1 (variable, consulta SLQ, etc)-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo del valor 1")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>8</length>
		<optionslist>Valor,Query,Campo</optionslist>
		<default>Valor</default>
	</field>

	<field>
		<name>tipo2</name>
		<!--Tipo del valor 2 (variable, consulta SLQ, etc)-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo del valor 2")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>8</length>
		<optionslist>Valor,Query,Campo</optionslist>
		<default>Valor</default>
	</field>
	
	<field>
		<name>tipocomp</name>
		<!--Indica cuál es el resultado que se considera correcto al hacer la comparación-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Resultado esperado")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>9</length>
		<optionslist>Iguales,Distintos,Mayor que,Menor que</optionslist>
		<default>Iguales</default>
	</field>
	
	<field>
		<name>accion</name>
		<!--Acción a realizar en el caso de que el resultado sea incorrecto-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Acción si hay fallo")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>8</length>
		<optionslist>Error,Saltar</optionslist>
		<default>Error</default>
	</field>
	
	<field>
		<name>valor1</name>
		<!--Valor del primer elemento a comparar-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Valor")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>

	<field>
		<name>valor2</name>
		<!--Valor del segundo elemento a comparar-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Valor")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>
	
	<field>
		<name>xml</name>
		<!--Nodo XML que almacena los datos de la comparación-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Xml")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>

</TMD>

