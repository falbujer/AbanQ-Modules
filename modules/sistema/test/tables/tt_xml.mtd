<!DOCTYPE TMD>
<TMD>
	<name>tt_xml</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Nodo XML")</alias>
	<!--Nodo XML que permite almacenar los valores de asignaciones y parámetros-->

	<field>
		<name>id</name>
		<!--Identificador interno de la tabla-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
	</field>

	<field>
		<name>xml</name>
		<!--Nodo xml que contiene la información de las asignaciones a realizar-->
		<alias>QT_TRANSLATE_NOOP("MetaData","xml")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>

</TMD>

