<!DOCTYPE TMD>
<TMD>
	<name>tt_tcatscat</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Pruebas por sesión")</alias>
	<!--Listado de las pruebas que forman cada una de las sesiones del catálogo de sesiones-->

	<field>
		<name>idtcatscat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<!--Identificador interno-->
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
		
		<relation>
			<table>tt_test</table>
			<field>idtcatscat</field>
			<card>1M</card>
		</relation>
	</field>

	<field>
		<name>testnumber</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Número")</alias>
		<!--Número que indica el orden de ejecución de la prueba dentro de la sesión-->
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>int</type>
		<counter>true</counter>
	</field>

	<field>
		<name>codtestcat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Prueba")</alias>
		<!--Identificador de la prueba asociada a la sesión-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
	</field>

	<field>
		<name>idtestcat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Prueba")</alias>
		<!--Identificador interno de la prueba asociada a la sesión-->
		<null>false</null>
		<pk>false</pk>
		<type>int</type>
		<visiblegrid>false</visiblegrid>
		
		<relation>
			<table>tt_testcat</table>
			<field>id</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>
	
	<field>
		<name>idsessioncat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Sesión")</alias>
		<!--Identificador interno de la sesión a la que se asocia la prueba-->
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>

		<relation>
			<table>tt_sessioncat</table>
			<field>id</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>

	<field>
		<name>codsessioncat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Sesión")</alias>
		<!--Identificador de la sesión a la que se asocia la prueba-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
	</field>
	
	<field>
		<name>description</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<!--Descripción de la prueba asociada a la sesión-->
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>200</length>
	</field>

	<field>
		<name>repeat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Repetir")</alias>
		<!--Número de veces que la prueba debe ser repetida durante la sesión-->
		<null>false</null>
		<pk>false</pk>
		<type>int</type>
		<default>1</default>
	</field>
	
	<field>
		<name>idfuncional</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Funcionalidad")</alias>
		<!--Funcionalidad de la prueba asociada a la sesión-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>
	</field>

	<field>
		<name>parameters</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Parámetros")</alias>
		<!--Parámetros necesarios para realizar la prueba-->
		<null>true</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>
</TMD>

