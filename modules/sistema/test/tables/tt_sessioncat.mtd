<!DOCTYPE TMD>
<TMD>
	<name>tt_sessioncat</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Catálogo de sesiones")</alias>
	<!--Listado de las sesiones de pruebas que pueden ser ejecutadas por el módulo-->

	<field>
		<name>id</name>
		<!-- Identificador interno -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
		
		<relation>
			<table>tt_tcatscat</table>
			<field>idsessioncat</field>
			<card>1M</card>
		</relation>
	</field>
	
	<field>
		<name>codsessioncat</name>
		<!--Código de la sesión-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Código")</alias>
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>string</type>
		<length>6</length>
		<counter>true</counter>
	</field>
	
	<field>
		<name>idfuncional</name>
		<!--Funcionalidad asociada a la sesión-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Funcionalidad")</alias>
		<null>true</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>string</type>
		<length>15</length>
	</field>
	
	<field>
		<name>description</name>
		<!--Descripción breve de las pruebas que se realizan en la sesión-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>200</length>
	</field>
	
</TMD>

