<!DOCTYPE TMD>
<TMD>
	<name>tt_stepcat</name>
	<alias>QT_TRANSLATE_NOOP("MetaData","Catálogo de pasos")</alias>
	<!--Listado de los pasos que pueden ser usados en las pruebas-->

	<field>
		<name>idstepcat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<!--Identificador interno-->
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>

		<relation>
			<table>tt_step</table>
			<field>idstepcat</field>
			<card>1M</card>
		</relation>
	</field>

	<field>
		<name>idtestcat</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Prueba asociada")</alias>
		<!--Identificador de la prueba asociada al paso en el catálogo de pruebas-->
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>uint</type>
		<visiblegrid>false</visiblegrid>

		<relation>
			<table>tt_testcat</table>
			<field>id</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
	</field>

	<field>
		<name>stepnumber</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Número")</alias>
		<!--Número que determina el orden de ejecución del paso en la prueba-->
		<null>false</null>
		<pk>false</pk>
		<ck>true</ck>
		<type>int</type>
		<counter>true</counter>
	</field>

	<field>
		<name>description</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<!--Descripción del paso-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>200</length>
	</field>

	<field>
		<name>container</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Contenedor")</alias>
		<!--Contenedor. Coincide con el nombre de la acción (de FacturaLUX, no del módulo de pruebas) que debe estar activa cuando el paso se ejecute-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>50</length>
	</field>

	<field>
		<name>idobjecttype</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo")</alias>
		<!--Tipo de objeto usado en el paso-->
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>50</length>

		<relation>
			<table>tt_objecttype</table>
			<field>idobjecttype</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>objectname</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Nombre")</alias>
		<!--Nombre del objeto usado en el paso-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>200</length>
	</field>

	<field>
		<name>idaction</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Acción")</alias>
		<!--Nombre de la acción que se realizará sobre el objeto seleccionado-->
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>50</length>

		<associated>
				<with>idobjecttype</with>
				<by>idobjecttype</by>
		</associated>

		<relation>
			<table>tt_action</table>
			<field>idaction</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>parameters</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Parámetros")</alias>
		<!--Parámetros asociados a la acción a realizar-->
		<null>true</null>
		<pk>false</pk>
		<type>stringlist</type>
	</field>

	<field>
		<name>idarea</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Área")</alias>
		<!--Área a la que pertenece el contenedor del paso-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>

		<relation>
			<table>flareas</table>
			<field>idarea</field>
			<card>M1</card>
		</relation>
	</field>
	
	<field>
		<name>idmodule</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Módulo")</alias>
		<!--Módulo al que pertenece el contenedor del paso-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>

		<associated>
				<with>idarea</with>
				<by>idarea</by>
		</associated>
		
		<relation>
			<table>flmodules</table>
			<field>idmodulo</field>
			<card>M1</card>
		</relation>
	</field> 
	
	<field>
		<name>formtype</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Tipo form.")</alias>
		<!--Tipo de formulario usado en el paso-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>15</length>
		<optionslist>form,formRecord,formSearch</optionslist>
		<default>form</default>
	</field>

	<field>
		<name>pbaccept</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Ok/Cancel")</alias>
		<!--Botón de la parte inferior derecha de los formularios de edición que se desea pulsar-->
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>25</length>
		<optionslist>Ninguno,Aceptar,Cancelar,Aceptar y continuar</optionslist>
		<default>Ninguno</default>
	</field>
</TMD>

