		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>aliasmodulo</name>
		<!-- Alias del Módulo -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Módulo")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
	</field>

	<field>
		<name>dependencias</name>
		<!-- Dependencias del módulo -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Dependencias")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
	</field>

	<field>
		<name>version</name>
		<!-- Dependencias del módulo -->
		<alias>QT_TRANSLATE_NOOP("MetaData","Versión")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
	</field>

</TMD>